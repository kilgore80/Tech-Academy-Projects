@using CarInsurance.ViewModel
@model List<QuotesVm>

<div class="row no-gutters m-4">
    <div class="card col-md-10  mx-auto shadow">
        <div class="card-header text-center">
            <h4 class="m-0">Customer Quotes</h4>
        </div>
        <div class="card-body">
            @foreach (var quote in Model)
            {
                <div class="row">
                    <div class="col">
                        @quote.FirstName
                    </div>
                    <div class="col">
                        @quote.LastName
                    </div>
                    <div class="col">
                        @quote.EmailAddress
                    </div>
                </div>
            }
        </div>
    </div>
</div>